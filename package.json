{
  "name": "conference",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "heroku-prebuild": "YARN_PRODUCTION=true",
    "heroku-postbuild": "cd client && yarn --only=dev && yarn && yarn run build",
    "start": "babel-node server.js",
    "client": "cd client && yarn start",
    "server": "nodemon --exec babel-node server.js",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "concurrently": "^4.1.2",
    "debug": "^4.1.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.2"
  },
  "engines": {
    "node": "10.16.3",
    "yarn": "1.17.3"
  }
}
